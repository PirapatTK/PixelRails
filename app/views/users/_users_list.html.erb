<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Birth Date</th>
      <th>Gender</th>
      <th>Email</th>
      <th>Phone Number</th>
      <th>Subject</th>
    </tr>
  </thead>
  <tbody id="users_list_tbody">
    <% @users.each do |user| %>
      <%= render 'user', user: user %>
    <% end %>
  </tbody>
</table>
