<div class="bg">
    <%= form_with model: @user, url: users_path, method: :post, data: { turbo_frame: 'user_form' }, class: 'container' do |userForm| %>
        <%= render 'form', form: userForm %>
        <%= userForm.submit 'Submit', class: 'btn-primary mt-xl'%>
    <% end %>

    <% @users.each do |user| %>
        <p><%= user.inspect%></p>
    <% end %>
</div>